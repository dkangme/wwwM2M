<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Sensor" table="Sensor">
    <indexes>
      <index name="fk_Sensor_WEI1_idx" columns="WEI_idWEI"/>
      <index name="fk_Sensor_SensorType1_idx" columns="SensorType_idSensorType"/>
    </indexes>
    <id name="idsensor" type="integer" column="idSensor">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="humanvalue" type="float" column="humanValue" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="value" type="integer" column="Value" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastreport" type="datetime" column="lastReport" nullable="false"/>
    <field name="description" type="string" column="Description" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="alarmlevel" type="integer" column="alarmLevel" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="formula" type="string" column="formula" length="45" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">%s*1</option>
      </options>
    </field>
    <field name="channelid" type="integer" column="channelId" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="savehistoryevery" type="integer" column="saveHistoryEvery" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">5</option>
      </options>
    </field>
    <many-to-one field="weiwei" target-entity="Wei" fetch="LAZY">
      <join-columns>
        <join-column name="WEI_idWEI" referenced-column-name="idWEI"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sensortypesensortype" target-entity="Sensortype" fetch="LAZY">
      <join-columns>
        <join-column name="SensorType_idSensorType" referenced-column-name="idSensorType"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
