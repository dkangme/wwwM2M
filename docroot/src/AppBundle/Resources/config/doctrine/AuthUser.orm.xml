<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AuthUser" table="auth_user">
    <indexes>
      <index name="IDX_A3B536FDD29AA452" columns="idCustomer"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="UNIQ_A3B536FD92FC23A8" columns="username_canonical"/>
      <unique-constraint name="UNIQ_A3B536FDA0D96FBF" columns="email_canonical"/>
      <unique-constraint name="UNIQ_A3B536FD7B495A69" columns="cell_phone"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usernameCanonical" type="string" column="username_canonical" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailCanonical" type="string" column="email_canonical" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="enabled" type="boolean" column="enabled" nullable="false"/>
    <field name="salt" type="string" column="salt" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastLogin" type="datetime" column="last_login" nullable="true"/>
    <field name="locked" type="boolean" column="locked" nullable="false"/>
    <field name="expired" type="boolean" column="expired" nullable="false"/>
    <field name="expiresAt" type="datetime" column="expires_at" nullable="true"/>
    <field name="confirmationToken" type="string" column="confirmation_token" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="passwordRequestedAt" type="datetime" column="password_requested_at" nullable="true"/>
    <field name="roles" type="array" column="roles" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="credentialsExpired" type="boolean" column="credentials_expired" nullable="false"/>
    <field name="credentialsExpireAt" type="datetime" column="credentials_expire_at" nullable="true"/>
    <field name="nombres" type="string" column="nombres" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidoP" type="string" column="apellido_p" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidoM" type="string" column="apellido_m" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cellPhone" type="string" column="cell_phone" length="16" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idcustomer" target-entity="Customer" fetch="LAZY">
      <join-columns>
        <join-column name="idCustomer" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
