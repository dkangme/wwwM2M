<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Sensortype" table="SensorType">
    <id name="idsensortype" type="integer" column="idSensorType">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="Name" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="Description" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="standard" type="string" column="Standard" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="minvalue" type="float" column="minValue" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="maxvalue" type="float" column="maxValue" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="measurement" type="string" column="measurement" length="45" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">PPM</option>
      </options>
    </field>
    <field name="cautionlevelformula" type="string" column="cautionLevelFormula" length="45" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">%s*1</option>
      </options>
    </field>
    <field name="warninglevelformula" type="string" column="warningLevelFormula" length="45" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">%s*1</option>
      </options>
    </field>
    <field name="alertlevelformula" type="string" column="alertLevelFormula" length="45" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">%s*1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
